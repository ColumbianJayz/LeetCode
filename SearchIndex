class Solution {
    public int searchIndex(int[] nums, int target) {
        int [] array= new int[nums.length];
        int tarjay = target;
        for(int i = 0; i < nums.length; i++){
            array[i] = nums[i];
        
            for(int babe : array){
                if(babe == tarjay){
                    return i;
                }
            }
        }
        return 0;
